{
  "version": 6,
  "include": [
    "base.json",
    "compilers/gcc.json",
    "configs/debug.json",
    "configs/release.json",
    "configs/bindings.json"
  ],
  "configurePresets": [
    {
      "name": "x86-linux-gcc-release",
      "inherits": [
        "base",
        "gcc",
        "release"
      ],
      "displayName": "Linux gcc release"
    },
    {
      "name": "x86-linux-gcc-debug",
      "inherits": [
        "base",
        "gcc",
        "debug"
      ],
      "displayName": "Linux gcc debug"
    },
    {
      "name": "x86-linux-gcc-release-bindings",
      "inherits": [
        "base",
        "gcc",
        "release",
        "bindings"
      ],
      "displayName": "Linux gcc release with bindings"
    }
  ],
  "buildPresets": [
    {
      "name": "x86-linux-gcc-release",
      "inherits": "base",
      "configurePreset": "x86-linux-gcc-release"
    },
    {
      "name": "x86-linux-gcc-debug",
      "inherits": "base",
      "configurePreset": "x86-linux-gcc-debug"
    },
    {
      "name": "x86-linux-gcc-release-bindings",
      "inherits": "base",
      "configurePreset": "x86-linux-gcc-release-bindings"
    }
  ],
  "testPresets": [
    {
      "name": "x86-linux-gcc-release",
      "inherits": "base",
      "configurePreset": "x86-linux-gcc-release"
    },
    {
      "name": "x86-linux-gcc-debug",
      "inherits": "base",
      "configurePreset": "x86-linux-gcc-debug"
    }
  ],
  "packagePresets": [
    {
      "name": "x86-linux-gcc-release",
      "inherits": "base",
      "configurePreset": "x86-linux-gcc-release"
    }
  ]
}