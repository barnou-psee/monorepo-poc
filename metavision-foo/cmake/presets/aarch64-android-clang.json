{
  "version": 6,
  "include": [
    "base.json",
    "toolchains/android-ndk.json",
    "configs/debug.json",
    "configs/release.json"
  ],
  "configurePresets": [
    {
      "name": "aarch64-android-clang",
      "hidden": true,
      "inherits": [
        "base",
        "android-ndk"
      ]
    },
    {
      "name": "aarch64-android-clang-debug",
      "inherits": [
        "aarch64-android-clang",
        "debug"
      ],
      "displayName": "Android clang debug"
    },
    {
      "name": "aarch64-android-clang-release",
      "inherits": [
        "aarch64-android-clang",
        "release"
      ],
      "displayName": "Android clang release"
    }
  ],
  "buildPresets": [
    {
      "name": "aarch64-android-clang-debug",
      "inherits": "base",
      "configurePreset": "aarch64-android-clang-debug"
    },
    {
      "name": "aarch64-android-clang-release",
      "inherits": "base",
      "configurePreset": "aarch64-android-clang-release"
    }
  ]
}